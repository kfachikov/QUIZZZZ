      </ImageView>
      <Label alignment="CENTER" layoutX="67.0" layoutY="5.0" prefHeight="35.0" prefWidth="469.0" text="THIS IS A MOCKUP" />
      <Label fx:id="gameIdLabel" alignment="CENTER" layoutY="46.0" prefHeight="355.0" prefWidth="600.0" text="Game Id unset" />
